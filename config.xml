<?xml version='1.0' encoding='utf-8'?>
<widget id="io.altoransystem.hostedWebApp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>hostedWebApp</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="https://www.geveret.co.il/" />
    <access origin="*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="~4.1.1" />
    <platform name="windows">
    </platform>
    <platform name="wp8">
    </platform>
    <platform name="android">
        <icon density="hdpi" src="res/mipmap-hdpi/ic_launcher.png" />
        <icon density="mdpi" src="res/mipmap-mdpi/ic_launcher.png" />
        <icon density="xhdpi" src="res/mipmap-xhdpi/ic_launcher.png" />
        <icon density="xxhdpi" src="res/mipmap-xxhdpi/ic_launcher.png" />
        <icon density="xxxhdpi" src="res/mipmap-xxxhdpi/ic_launcher.png" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-hostedwebapp" spec="~0.3.1" />
    <plugin name="cordova-plugin-vibration" spec="~2.1.4" />
    <plugin name="de.appplant.cordova.plugin.local-notification" spec="~0.8.4" />
    <plugin name="cordova-plugin-dialogs" spec="~1.3.2" />
    <allow-navigation hap-rule="yes" href="https://www.geveret.co.il/*" />
    <platform name="android">
    <preference name="android-targetSdkVersion" value="28" />
</platform>
</widget>
